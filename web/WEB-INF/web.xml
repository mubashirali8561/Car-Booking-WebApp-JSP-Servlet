        <servlet-name>UpdateStatus</servlet-name>
        <servlet-class>com.system.controller.UpdateStatus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Booking</servlet-name>
        <servlet-class>com.system.controller.Booking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookCar</servlet-name>
        <servlet-class>com.system.controller.BookCar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateCar</servlet-name>
        <servlet-class>com.system.controller.UpdateCar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Discount</servlet-name>
        <servlet-class>com.system.controller.Discount</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCar</servlet-name>
        <url-pattern>/AddCar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCars</servlet-name>
        <url-pattern>/AddCars</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCar</servlet-name>
        <url-pattern>/DeleteCar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetPassword</servlet-name>
        <url-pattern>/ResetPassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStatus</servlet-name>
        <url-pattern>/UpdateStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Booking</servlet-name>
        <url-pattern>/Booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookCar</servlet-name>
        <url-pattern>/BookCar</url-pattern>
    </servlet-mapping>
