        <servlet-name>UpdateCar</servlet-name>
        <url-pattern>/UpdateCar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Discount</servlet-name>
        <url-pattern>/Discount</url-pattern>
    </servlet-mapping>
